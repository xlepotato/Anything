@model List<Currency>
@{
    Layout = "/Views/Shared/HeaderLogoOnly.cshtml";
}



            <!--Left Container-->
         
                <div class="currencyInfoContainer form-group col-xs-4 col-md-4" id="leftContainer">
                    <div class="drawBorder">
                        <input class="currencyInputTF" id="tbExchangeFrom" type="number" min="0.00" max="9999" value="1.00">
                        <select class="form-control exchangeDropdown btn dropdown-toggle" id="selExchangeFrom">
                            @foreach (var x in Model)
                            {
                                <option>@x.Name</option>
                            }
                        </select>
                    </div>                
                </div>
                 @*@foreach (var x in Model)
                {
                     <h5><img src="@x.Flag" /></h5>
                    <h5>@x.Name</h5>
                }*@
                <!--Mid Container-->
                <div class="currencyInfoContainer form-group col-md-4" id="midContainer">
                    <img src="~/Assets/Images/exchangeicon.jpg" id="exchangeIcon" />
                </div>
                <!--Right Container-->
                <div class="currencyInfoContainer form-group col-md-4" id="rightContainer" ;">
                    <div class="drawBorder">
                        <input class="currencyInputTF" id="tbExchangeTo" type="number" min="0.00" max="9999" value="1.00">
                        <select class="form-control exchangeDropdown" id="selExchangeTo">
                            @foreach (var x in Model)
                            {
                                <option>@x.Name</option>
                            }
                        </select>
                    </div>
                </div>
            </div>
            <div class="col-md-12">
                <div class="putCenter" id="MarketRateDiv">Market Rate: <label id="lblResult">1.00</label></div>
            </div>
            <div class="col-md-12"> <div class="putCenter"><button type="button" class="btn btn-primary btn-lg" id="btnViewMoneyChangers">View Money Changers</button></div></div>
         

           
      

